{
  "extends": "./.svelte-kit/tsconfig.json",
  "paths": {
    "$lib": ["lib"],
    "$lib/*": ["lib/*"],
    "$routes": ["routes"],
    "$routes/*": ["routes/*"]
  },
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true
  }
}
